   "source": [
    "import aspose.pdf as ap\n",
    "\n",
    "input_pdf = \"input.pdf\"\n",
    "output_tex = \"pdf_to_tex.tex\"\n",
    "\n",
    "# Open PDF file\n",
    "document = ap.Document(input_pdf)\n",
    "\n",
    "# Create an object of LaTeXSaveOptions class\n",
    "saveOptions = ap.LaTeXSaveOptions()\n",
    "\n",
    "# Save PDF as TEX\n",
    "document.save(output_tex, saveOptions)"
   ]
